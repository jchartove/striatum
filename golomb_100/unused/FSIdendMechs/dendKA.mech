vk = [-90]
gka = [64]
thetama = [-45]
sigmama = [-13]
thetaha = [-77]
sigmaha = [-8]
sigmatma = [-13]
phima = [-70]
 
gka_dend = gka/10
 
minf(X) = 1./(1+exp(-(X-thetama)./sigmama))
hinf(X) = 1./(1+exp(-(X-thetaha)./sigmaha)) 
taum(X) = 1+exp(-(X-phima)./sigmatma) 
dendKA(X,m,h) = gka_dend.*h.*(m.^4).*(X-vk)

m' = (minf(X)-h)./taum(X);
m(0) = 0
h' = (hinf(X)-h)./14;
h(0) = 0
 
@current += -dendKA(X,m,h)

 
